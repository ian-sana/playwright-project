{
  "compilerOptions": {
    
    "target": "es2016",
    "types": ["node"],
    "baseUrl": "./",
    "paths": {
      "@config": ["playwright.config"],
      "@/fixtures/*": ["fixtures/*"],
      "@basePage": ["page-objects/base.page"],
      "@/page-objects/*": ["page-objects/*"],
      "@baseAPI": ["api-objects/clients/base.api"],
      "@/api-objects/*": ["api-objects/*"],
      "@/api-objects/clients/*": ["api-objects/clients/*"],
      "@/api-objects/models/*": ["api-objects/models/*"],
      "@/testData/*": ["test-data/*"],
      "@/utils/*": ["utils/*"],
      "@/enums/*": ["enums/*"],
      "@/componnts/*": ["componnts/*"]
    },
    "strict": true,
    "module": "commonjs",
    "noEmit": true, 
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["resources", "tests", "playwright.config.ts", "enums", "fixtures", "page-objects", "api-objects", "test-data", "utils", "componnts"]
}
